<openerp>
    <data>
        <template id="vert_stock_move_product_report">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="data">
                    <t t-call="report.external_layout">
                        <div class="page">
                            <table class="table table-condensed table-bordered">
                                <thead>
                                    <tr>
                                        <th rowspan="2">Date</th>
                                        <th rowspan="2">Source</th>
                                        <th rowspan="2">Supplier</th>
                                        <th rowspan="2">Product</th>
                                        <th rowspan="2">Total Quantity Ordered</th>
                                        <th rowspan="2">Total Quantity Received</th>
                                        <th colspan="2">Total Quantity Pending</th>
                                        <th rowspan="2">GIT Vessel</th>
                                        <th rowspan="2">Status</th>
                                        <th rowspan="2">Date Received</th>
                                        <th rowspan="2">GIT Container</th>
                                        <th rowspan="2">GIT ETA</th>
                                        <th rowspan="2">GIT QTY</th>
                                        <th rowspan="2">1.Client</th>
                                        <th rowspan="2">1.QTY</th>
                                        <th rowspan="2">1.SO Ref</th>
                                        <th rowspan="2">Total Outgoing Qty</th>
                                        <th rowspan="2">Unallocated Available</th>
                                    </tr>
                                    <tr>
                                        <th>Total Quantity Goods In Transit</th>
                                        <th>Total Quantity to Be Produced</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><span t-field="data.date"/></td>
                                        <td><span t-field="data.origin"/></td>
                                        <td><span t-field="data.picking_id.partner_id.name"/></td>
                                        <td><span t-field="data.product_id"/></td>
                                        <td><span t-field="data.purchase_line_id.invoice_lines.quantity"/></td>
                                        <td><span t-field="data.quant_ids.qty"/></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td><span t-field="data.state"/></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</openerp>
